{
  "name": "insmed",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "start": "react-native start",
    "test": "jest",
    "build:staging": "cd ./android && ./gradlew assembleRelease",
    "publish:staging": "appcenter codepush release-react --app andressrg/insmed",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/insmed/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'"
  },
  "dependencies": {
    "expo": "~36.0.0",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "~0.61.4",
    "react-native-ble-plx": "^1.1.1",
    "react-native-code-push": "^6.1.1",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-reanimated": "~1.4.0",
    "react-native-screens": "2.0.0-alpha.12",
    "react-native-svg": "^12.0.3",
    "react-native-unimodules": "~0.7.0",
    "react-native-web": "~0.11.7",
    "victory-native": "^34.1.0"
  },
  "devDependencies": {
    "@babel/core": "~7.6.0",
    "@types/react": "~16.9.0",
    "@types/react-native": "~0.60.23",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "babel-preset-expo": "~8.0.0",
    "eslint": "6.x",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "jest-expo": "~36.0.1",
    "typescript": "~3.6.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "private": true,
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  }
}
